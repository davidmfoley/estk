{
  "name": "estk-events-in-memory",
  "version": "0.0.1",
  "description": "",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run -s test:lint && npm run -s test:mocha",
    "test:mocha": "mocha --recursive --compilers js:babel-register test",
    "test:lint": "eslint src/",
    "build": "npm run -s build:clean && npm run -s build:lib && npm run build:flow",
    "build:clean": "rimraf lib",
    "build:lib": "babel -d lib --ignore '**/*.test.js' src",
    "build:flow": "flow-copy-source -v -i '**/*.test.js' src lib",
    "build-and-test": "npm run -s build && npm run -s test:mocha",
    "prepublishOnly": "npm run -s build"
  },
  "dependencies": {
    "debug": "^2.6.4"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "eslint": "^3.12.2",
    "flow": "^0.2.3",
    "flow-bin": "^0.37.4",
    "flow-copy-source": "^1.1.0",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4"
  },
  "author": "Dave Foley",
  "license": "MIT"
}
