{
  "name": "estk-events",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run -s test:lint && npm run -s test:flow && npm run -s test:mocha",
    "test:mocha": "mocha --recursive --compilers js:babel-register test",
    "test:lint": "eslint src/",
    "build": "npm run -s build:clean && npm run -s build:lib",
    "build:clean": "rimraf lib",
    "build:lib": "babel -d lib --ignore '**/*.test.js' src",
    "build:flow": "flow-copy-source -v -i '**/*.test.js' src lib",
    "build-and-test": "npm run -s test:mocha && npm run -s build",
    "prepublishOnly": "npm run -s build"
  },
  "dependencies": {
    "debug": "^2.6.4",
    "microtime": "^2.1.5",
    "pg": "^6.1.4",
    "pg-pool": "^1.7.1",
    "estk-events": "0.0.1"
  },
  "devDependencies": {
    "babel-eslint": "7",
    "chai": "^3.5.0",
    "eslint": "^3.12.2",
    "eslint-plugin-flowtype": "^2.42.0",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4"
  },
  "author": "Dave Foley",
  "license": "MIT"
}
