{
  "name": "estk",
  "private": true,
  "scripts": {
    "flow": "! flow check 2>/dev/null | grep -v 'Found 0 errors' 1>&2",
    "test": "bin/run-for-all 'yarn test'",
    "cover": "bin/run-for-all '../node_modules/.bin/nyc yarn test'",
    "build": "bin/run-for-all 'yarn run build'",
    "publish": "npm run -s build && bin/run-for-all npm publish",
    "link-all": "bin/run-for-all 'yarn run link'",
    "ci": "npm run -s build && npm run -s test && npm run -s flow"
  },
  "workspaces": [
    "estk-*"
  ],
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "7",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "chai": "^4.1.2",
    "eslint": "^3.12.2",
    "eslint-plugin-flowtype": "^2.42.0",
    "flow-bin": "^0.81.0",
    "flow-copy-source": "^1.1.0",
    "flow-typed": "^2.2.3",
    "mocha": "^3.2.0",
    "nyc": "^11.6.0",
    "rimraf": "^2.5.4",
    "sinon": "^4.5.0",
    "sinon-chai": "^3.0.0",
    "watchr": "^4.0.1"
  },
  "dependencies": {},
  "version": "0.0.2"
}
