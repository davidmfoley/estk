{
  "name": "estk",
  "private": true,
  "scripts": {
    "flow": "! flow check 2>/dev/null | grep -v 'Found 0 errors' 1>&2",
    "test": "bin/run-for-all 'yarn test'",
    "cover": "bin/run-for-all '../node_modules/.bin/nyc yarn test'",
    "build": "bin/run-for-all 'yarn run build'",
    "link-all": "bin/run-for-all 'yarn link'",
    "watch": "bin/watch",
    "ci": "npm run -s build && npm run -s test && npm run -s flow"
  },
  "workspaces": [
    "estk-*"
  ],
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.4",
    "@types/sinon-chai": "^3.2.2",
    "@typescript-eslint/parser": "^2.0.0",
    "chai": "^4.1.2",
    "eslint": "^6.8.0",
    "mocha": "6.2.0",
    "nyc": "^15.0.0",
    "rimraf": "^2.5.4",
    "sinon": "^4.5.0",
    "sinon-chai": "^3.0.0",
    "watchr": "^4.0.1"
  },
  "dependencies": {
    "prettier": "^1.18.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.7.4"
  },
  "version": "0.0.3"
}
